<h1>INDEX</h1>
<h2><%=@title%></h2>

<ul>
	<% @langs.each do |lang| %>
	<li><%=lang %></li>
	<% end %>
	
</ul>

<table>
	<tr>
		<td>id</td>
		<td>title</td>
		<td>page</td>
		<td>description</td>
	</tr>
	<% @books.each do |book| %>
	<tr>
		<td><%= book.id %></td>
		<td><%= book.title %></td>
		<td><%= book.page %></td>
		<td><%= book.description %></td>
	</tr>
	<% end %>
</table>